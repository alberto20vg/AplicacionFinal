<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ui.datos.DatosFragment">

    <ImageButton
        android:id="@+id/btnPlatos"
        android:layout_width="340dp"
        android:layout_height="220dp"
        android:layout_marginTop="50dp"
        android:background="#FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/foto_platos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btnIngredientes"
        android:layout_width="347dp"
        android:layout_height="295dp"
        android:layout_marginStart="56dp"
        android:background="#FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/foto_ingredientes"
        app:layout_constraintBottom_toBottomOf="@+id/btnPlatos"
        app:layout_constraintStart_toEndOf="@+id/btnPlatos"
        app:layout_constraintTop_toTopOf="@+id/btnPlatos" />
</androidx.constraintlayout.widget.ConstraintLayout>